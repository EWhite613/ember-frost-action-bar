{
  "name": "ember-frost-action-bar",
  "version": "0.0.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ciena-frost/ember-frost-action-bar",
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
